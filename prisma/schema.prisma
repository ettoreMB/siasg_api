generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model sysdiagrams {
  name         String @db.NVarChar(128)
  principal_id Int
  diagram_id   Int    @id(map: "PK__sysdiagr__C2B05B6135E9D815") @default(autoincrement())
  version      Int?
  definition   Bytes?

  @@unique([principal_id, name], map: "UK_principal_name")
}

model TB_CATMAT_CLASSES {
  Id               Decimal          @unique(map: "IX_TB_CATMAT_CLASSES_Id") @db.Decimal(18, 0)
  Cod_Grupo        String           @db.VarChar(2)
  Cod_Classe       String           @id(map: "PK_TB_CATMAT_CLASSES") @db.VarChar(4)
  Classe           String           @db.VarChar(512)
  Insert_Date      DateTime?        @default(dbgenerated("[dbo].[MyDate]()"), map: "DF_TB_CATMAT_CLASSES_Insert_Date") @db.DateTime
  Insert_User      String?          @db.VarChar(256)
  Update_Date      DateTime?        @db.DateTime
  Update_User      String?          @db.VarChar(256)
  TB_CATMAT_GRUPOS TB_CATMAT_GRUPOS @relation(fields: [Cod_Grupo], references: [Cod_Grupo], onUpdate: NoAction, map: "FK_TB_CATMAT_CLASSES_TB_CATMAT_GRUPOS")
  TB_CATMAT_PDM    TB_CATMAT_PDM[]

  @@index([Cod_Grupo], map: "IX_TB_CATMAT_CLASSES_Cod_Grupo")
}

model TB_CATMAT_GRUPOS {
  Id                Decimal             @unique(map: "IX_TB_CATMAT_GRUPOS_Id") @db.Decimal(18, 0)
  Cod_Grupo         String              @id(map: "PK_TB_CATMAT_GRUPOS") @db.VarChar(2)
  Grupo             String              @unique(map: "IX_TB_CATMAT_GRUPOS_Grupo") @db.VarChar(512)
  Insert_Date       DateTime?           @default(dbgenerated("[dbo].[MyDate]()"), map: "DF_TB_CATMAT_GRUPOS_Insert_Date") @db.DateTime
  Insert_User       String?             @db.VarChar(256)
  Update_Date       DateTime?           @db.DateTime
  Update_User       String?             @db.VarChar(256)
  TB_CATMAT_CLASSES TB_CATMAT_CLASSES[]
}

model TB_CATMAT_MATERIAIS {
  Id            Decimal       @unique(map: "IX_TB_CATMAT_MATERIAIS") @db.Decimal(18, 0)
  Cod_PDM       String        @db.VarChar(5)
  CATMAT        String        @id(map: "PK_TB_CATMAT_MATERIAIS") @db.VarChar(6)
  Material      String        @db.VarChar(2048)
  Status        String        @default("SIM", map: "DF_TB_CATMAT_MATERIAIS_Status") @db.VarChar(16)
  Sustentavel   String?       @db.VarChar(32)
  Insert_Date   DateTime?     @default(dbgenerated("[DBO].[MYDATE]()"), map: "DF_TB_CATMAT_MATERIAIS_Insert_Date") @db.DateTime
  Insert_User   String?       @db.VarChar(256)
  Update_Date   DateTime?     @db.DateTime
  Update_User   String?       @db.VarChar(256)
  TB_CATMAT_PDM TB_CATMAT_PDM @relation(fields: [Cod_PDM], references: [Cod_PDM], onUpdate: NoAction, map: "FK_TB_CATMAT_MATERIAIS_TB_CATMAT_PDM")
}

model TB_CATMAT_PDM {
  Id                  Decimal               @unique(map: "IX_TB_CATMAT_PDM_Id") @db.Decimal(18, 0)
  Cod_Classe          String                @db.VarChar(4)
  Cod_PDM             String                @id(map: "PK_TB_CATMAT_PDM") @db.VarChar(5)
  PDM                 String                @unique(map: "IX_TB_CATMAT_PDM_PDM") @db.VarChar(512)
  Insert_Date         DateTime?             @default(dbgenerated("[dbo].[MyDate]()"), map: "DF_TB_CATMAT_PDM_Insert_Date") @db.DateTime
  Insert_User         String?               @db.VarChar(256)
  Update_Date         DateTime?             @db.DateTime
  Update_User         String?               @db.VarChar(256)
  TB_CATMAT_CLASSES   TB_CATMAT_CLASSES     @relation(fields: [Cod_Classe], references: [Cod_Classe], onUpdate: NoAction, map: "FK_TB_CATMAT_PDM_TB_CATMAT_CLASSES")
  TB_CATMAT_MATERIAIS TB_CATMAT_MATERIAIS[]
}

model TB_SIASG_COMPRAS {
