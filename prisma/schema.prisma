  CATMAT               String?   @db.VarChar(100)
  ATUALIZACAO          String?   @db.VarChar(100)
  CODIGO_BR            String?   @db.VarChar(100)
  DESCRICAO_CATMAT     String?   @db.VarChar(Max)
  UNIDADE_FORNECIMENTO String?   @db.VarChar(100)
  GENERICO             String?   @db.VarChar(100)
  REGISTRO_ANVISA      String?   @db.VarChar(100)
  DATA_COMPRA          String?   @db.VarChar(100)
  MODALIDADE_COMPRA    String?   @db.VarChar(100)
  DATA_INSERCAO        String?   @db.VarChar(100)
  TIPO_COMPRA          String?   @db.VarChar(100)
  FABRICANTE           String?   @db.VarChar(100)
  CNPJ_FABRICANTE      String?   @db.VarChar(100)
  FORNECEDOR           String?   @db.VarChar(100)
  CNPJ_FORNECEDOR      String?   @db.VarChar(100)
  QUALIFICACAO         String?   @db.VarChar(100)
  NOME_INSTITUICAO     String?   @db.VarChar(100)
  CNPJ_INSTITUICAO     String?   @db.VarChar(100)
  MUNICIPIO            String?   @db.VarChar(100)
  UF                   String?   @db.VarChar(100)
  QTD_ITENS_COMPRADOS  Decimal?  @db.Decimal(18, 2)
  PRECO_UNITARIO       Decimal?  @db.Decimal(18, 2)
  MEDIA_PONDERADA      Decimal?  @db.Decimal(18, 2)
  TOTAL                Decimal?  @db.Decimal(18, 2)
  ANO_MERCADO          Decimal?  @db.Decimal(18, 0)
  IMPORTADO_POR        String?   @db.VarChar(100)
  DATA_IMPORTACAO      DateTime? @default(now(), map: "DF_TB_SIASG_COMPRAS_DATA_IMPORTACAO") @db.Date
